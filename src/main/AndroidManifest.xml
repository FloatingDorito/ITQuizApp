<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="student.inti.itquizapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ITQuizApp">
        <activity android:name=".Profile"
            android:label="@string/profile_name"></activity>
        <activity android:name=".QuizResult" />
        <activity android:name=".BeginQuiz" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".RegisterPlayer" />
        <activity android:name=".QuizMain"
            android:label="@string/quiz_name"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>